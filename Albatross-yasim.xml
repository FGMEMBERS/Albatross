      incidence="2" 
      twist="-3">
  <stall aoa="14" width="6" peak="1.5"/>
  <flap0 start="0.019" end="0.42" lift="1.4" drag="1.4"/>
  <flap1 start="0.51" end="0.92" lift="1.4" drag="1.2"/>
